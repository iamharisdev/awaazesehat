{
  "expo": {
     "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 500
        }
      ]
    ],
    "name": "awaazesehat-app",
    "slug": "awaazesehat-app",
    "scheme": "awaazesehat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#0B6E27",
       "imageWidth": 500,
       "imageHeight":1000,
      "width":500,
      "hight":1000
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B6E27"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.awaazesehat123.awaazesehatapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
